{
  "name": "backend",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy && prisma db push",
    "setup": "ts-node-dev -r tsconfig-paths/register src/scripts/run-generate-config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "simple-git": "^3.27.0",
    "tsconfig-paths": "^4.2.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.5",
    "prisma": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.15",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
